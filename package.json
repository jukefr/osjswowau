{
  "name": "osjswowau",
  "repository": {
    "type": "git",
    "url": "https://github.com/jukefr/osjswowau.git"
  },
  "description": "üïπÔ∏è a cross-platform cli utility to update your World of Warcraft addons",
  "version": "4.0.4",
  "main": "index.js",
  "license": "GPL-3.0",
  "bin": "./osjswowau",
  "scripts": {
    "start": "./osjswowau",
    "build:linux64": "nexe -t linux-x64-12.9.1 -o dist/osjswowau-linux64 index.js",
    "build:linux86": "nexe -t linux-x86-12.9.1 -o dist/osjswowau-linux86 index.js",
    "build:windows64": "nexe -t windows-x64-12.9.1 -o dist/osjswowau-windows64 index.js",
    "build:windows86": "nexe -t windows-x86-12.9.1 -o dist/osjswowau-windows86 index.js",
    "build:mac64": "nexe -t mac-x64-12.9.1 -o dist/osjswowau-mac64 index.js",
    "lint": "prettier -w ./*.js",
    "changelog": "auto-changelog -p",
    "release": "dotenv release-it --"
  },
  "keywords": [
    "wow",
    "addon",
    "updater",
    "shadowlands",
    "blizzard"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "cli-progress": "^3.8.2",
    "conf": "^7.1.2",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "md5-file": "^5.0.0",
    "moment": "^2.29.1",
    "puppeteer-cluster": "^0.22.0",
    "puppeteer-core": "^5.5.0",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-stealth": "^2.6.5",
    "unzipper": "^0.10.11"
  },
  "resolutions": {
    "clone-deep": "4.0.1"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "nexe": "^3.3.7",
    "prettier": "^2.2.1",
    "release-it": "^14.2.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "hooks": {
      "before:init": [],
      "after:bump": [
        "npm run lint",
        "npm run changelog",
        ["npm run build:linux64",
        "npm run build:linux86",
        "npm run build:windows64",
        "npm run build:linux86",
        "npm run build:mac64"]
      ]
    },
    "github": {
      "release": true,
      "assets": [
        "dist/osjswowau*"
      ]
    },
    "npm": {
      "publish": true
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "eslintConfig": {
    "rules": {
      "no-console": "off",
      "no-await-in-loop": "off",
      "no-underscore-dangle": "off",
      "no-restricted-syntax": "off"
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    }
  }
}
